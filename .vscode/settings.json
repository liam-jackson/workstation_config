        "**/*gtk3*": false,
        "config/zsh/.bash_aliases": false,
        "config/zsh/.bash_func_defs": false,
        "config/zsh/.bash_logout": false,
        "config/zsh/.bash_history_config": false,
        "config/zsh/.bash_colors_config": false,
        "config/zsh/.bashrc": false,
        "config/zsh/.bashrc_ORIGINAL": false,
        "config/zsh/antigen.zsh": false,
        "config/dconf/user": false
    }

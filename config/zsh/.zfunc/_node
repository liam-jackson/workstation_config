#compdef node

_node_complete() {
  local cur_word options
  cur_word="${COMP_WORDS[COMP_CWORD]}"
  if [[ "${cur_word}" == -* ]]; then
    COMPREPLY=($(compgen -W '--experimental-sea-config --preserve-symlinks-main --allow-fs-write --test-udp-no-try-send --tls-max-v1.3 --pending-deprecation --dns-result-order --test-only --tls-min-v1.2 --force-context-aware --max-semi-space-size --perf-prof-unwinding-info --expose-internals --warnings --experimental-network-imports --check --tls-min-v1.1 --addons --node-memory-debug --experimental-specifier-resolution --prof-process --trace-warnings --force-async-hooks-checks --diagnostic-dir --deprecation --inspect-brk-node --tls-min-v1.0 --insecure-http-parser --watch-preserve-output --heapsnapshot-near-heap-limit --disable-warning --heapsnapshot-signal --experimental-wasi-unstable-preview1 --interactive --input-type --jitless --experimental-repl-await --stack-trace-limit --extra-info-on-fatal-exception --watch --experimental-default-type --trace-deprecation --eval --experimental-test-coverage --disallow-code-generation-from-strings --title --throw-deprecation --experimental-vm-modules --global-search-paths --experimental-global-customevent --prof --heap-prof --experimental-json-modules --openssl-shared-config --cpu-prof-interval --heap-prof-interval --security-revert --watch-path --tls-cipher-list --test-timeout --disable-proto --env-file --cpu-prof --zero-fill-buffers --require --experimental-top-level-await --openssl-legacy-provider --v8-pool-size --force-fips --experimental-shadow-realm --preserve-symlinks --completion-bash --experimental-global-webcrypto --secure-heap-min --abort-on-uncaught-exception --trace-event-categories --v8-options --report-dir --unhandled-rejections --snapshot-blob --report-on-fatalerror --huge-max-old-generation-size --node-snapshot --icu-data-dir --trace-uncaught --openssl-config --experimental-abortcontroller --report-filename --experimental-permission --interpreted-frames-native-stack --experimental-import-meta-resolve --experimental-worker --use-bundled-ca --test-shard --test-name-pattern --use-largepages --test-reporter --secure-heap --experimental-loader --debug-brk --trace-tls --tls-max-v1.2 --test-concurrency --perf-prof --force-node-api-uncaught-exceptions-policy --perf-basic-prof-only-functions --report-compact --debug-arraybuffer-allocations --trace-sync-io --trace-event-file-pattern --test-reporter-destination --heap-prof-dir --max-http-header-size --redirect-warnings --enable-etw-stack-walking --perf-basic-prof --max-old-space-size --allow-worker --print --report-signal --network-family-autoselection --tls-keylog --cpu-prof-name --report-uncaught-exception --trace-exit --harmony-shadow-realm --heap-prof-name --version --frozen-intrinsics --inspect-brk --inspect-publish-uid --napi-modules --experimental-modules --experimental-report --allow-child-process --allow-fs-read --build-snapshot --debug --verify-base-objects --trace-atomics-wait --policy-integrity --experimental-detect-module --trace-sigint --import --test --help --conditions --report-on-signal --cpu-prof-dir --inspect --use-openssl-ca --experimental-wasm-modules --inspect-port --enable-fips --experimental-websocket --tls-min-v1.3 --experimental-policy --track-heap-objects --experimental-fetch --http-parser --enable-source-maps --debug-port -i --inspect= --print <arg> --debug= --debug-brk= --inspect-brk= -C --enable-network-family-autoselection -pe --loader --prof-process -c --inspect-brk-node= -e -r --trace-events-enabled -v --security-reverts --es-module-specifier-resolution -h -p --report-directory' -- "${cur_word}"))
    return 0
  else
    COMPREPLY=($(compgen -f "${cur_word}"))
    return 0
  fi
}
complete -o filenames -o nospace -o bashdefault -F _node_complete node node_g
